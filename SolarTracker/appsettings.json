{
  "AppSettings": {
    "Auto": true,
    "AutoInterval": "0.00:15:00.000"
  },

  "DeviceSettings": {
    "Latitude": 0,
    "Longitude": 0,

    "MinAzimuth": 0,
    "MaxAzimuth": 0,
    "MinAltitude": 0,
    "MaxAltitude": 0
  },

  "IpGeolocationClientSettings": {
    "ApiKey": "secret"
  },

  "Serilog": {
    "LevelSwitches": { "$controlSwitch": "Information" },
    "MinimumLevel": { "ControlledBy": "$controlSwitch" },
    "WriteTo": [
      {
        "Name": "Seq",
        "Args": {
          "controlLevelSwitch": "$controlSwitch",
          "serverUrl": "secret",
          "apiKey": "secret"
        }
      }
    ],
    "Properties": {
      "app": "SolarTracker"
    }
  },

  "AllowedHosts": "*"

}