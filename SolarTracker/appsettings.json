{
  "AppSettings": {
    "Auto": true,
    "AutoInterval": "0.00:15:00.000",

    "AzimuthMinAngleForDrive": 1,
    "AltitudeMinAngleForDrive": 1
  },

  "DeviceSettings": {
    "Latitude": 0,
    "Longitude": 0,

    "MinAzimuth": 0,
    "MinAzimuthLimitPin": 4,
    "MaxAzimuth": 0,
    "MaxAzimuthLimitPin": 17,
    "AzimuthDriveNegativePin": 5,
    "AzimuthDrivePositivePin": 6,

    "MinAltitude": 0,
    "MinAltitudeLimitPin": 27,
    "MaxAltitude": 0,
    "MaxAltitudeLimitPin": 22,
    "AltitudeDriveNegativePin": 12,
    "AltitudeDrivePositivePin": 13
  },

  "IpGeolocationClientSettings": {
    "ApiKey": "secret"
  },

  "Serilog": {
    "LevelSwitches": { "$controlSwitch": "Information" },
    "MinimumLevel": { "ControlledBy": "$controlSwitch" },
    "WriteTo": [
      {
        "Name": "Seq",
        "Args": {
          "controlLevelSwitch": "$controlSwitch",
          "serverUrl": "secret",
          "apiKey": "secret"
        }
      }
    ],
    "Properties": {
      "app": "SolarTracker"
    }
  },

  "AllowedHosts": "*"

}